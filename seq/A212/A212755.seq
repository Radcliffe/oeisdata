%t A212755 t = Compile[{{n, _Integer}}, Module[{s = 0},
%t A212755 (Do[If[Abs[w - x] == Max[w, x, y, z] - Min[w, x, y, z], s = s + 1],
%t A212755 {w, 0, n}, {x, 0, n}, {y, 0, n}, {z, 0, n}]; s)]];
%t A212755 Map[t[#] &, Range[0, 40]]   (* A212755 *)
