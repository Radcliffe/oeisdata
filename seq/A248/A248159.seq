%p A248159 A248159:=n->(-1)^(n+1)*(n+1)*(n^2-7*n-6)/3!: seq(A248159(n),n=0..50); # _Wesley Ivan Hurt_, Oct 07 2014
%t A248159 Table[(-1)^(n + 1)*(n + 1)*(n^2 - 7*n - 6)/3!, {n, 0, 50}] (* _Wesley Ivan Hurt_, Oct 07 2014 *)
