%t A324337 Block[{f}, f[m_] := Module[{a = 1, b = 0, n = m}, While[n > 0, If[OddQ[n], b = a + b, a = a + b]; n = Floor[n/2]]; b]; Array[f@ Fold[BitXor, #, Quotient[#, 2^Range[BitLength[#] - 1]]] &, 106, 0]] (* _Michael De Vlieger_, Dec 14 2019, after _Jean-FranÃ§ois Alcover_ at A002487 and _Jan Mangaldan_ at A006068 *)
%o A324337 (PARI)
%o A324337 A006068(n)= { my(s=1, ns); while(1, ns = n >> s; if(0==ns, break()); n = bitxor(n, ns); s <<= 1; ); return (n); } \\ From A006068
%o A324337 A002487(n) = { my(s=sign(n), a=1, b=0); n = abs(n); while(n>0, if(bitand(n, 1), b+=a, a+=b); n>>=1); (s*b); };
%o A324337 A324337(n) = A002487(A006068(n));
