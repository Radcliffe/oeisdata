%p A003622 A003622 := proc(n)
%p A003622     n+floor(n*(1+sqrt(5))/2)-1 ;
%p A003622 end proc: # _R. J. Mathar_, Jan 25 2015
%p A003622 # Maple code for the Wythoff compound sequences, from _N. J. A. Sloane_, Mar 30 2016
%p A003622 # The Wythoff compound sequences: Let A = A000201, B = A001950. Then AA = A003622, AB = A003623, BA = A035336, BB = A101864. The eight triples AAA, AAB, ..., BBB are A134859, A134860, A035337, A134862, A134861, A134863, A035338, A134864, resp.
%p A003622 # Assume files out1, out2 contain lists of the terms in the base sequences A and B from their b-files
%p A003622 read out1; read out2; b[0]:=b1: b[1]:=b2:
%p A003622 w2:=(i,j,n)->b[i][b[j][n]];
%p A003622 w3:=(i,j,k,n)->b[i][b[j][b[k][n]]];
%p A003622 for i from 0 to 1 do
%p A003622 lprint("name=",i);
%p A003622 lprint([seq(b[i][n],n=1..100)]):
%p A003622 od:
%p A003622 for i from 0 to 1 do for j from 0 to 1 do
%p A003622 lprint("name=",i,j);
%p A003622 lprint([seq(w2(i,j,n),n=1..100)]);
%p A003622 od: od:
%p A003622 for i from 0 to 1 do for j from 0 to 1 do for k from 0 to 1 do
%p A003622 lprint("name=",i,j,k);
%p A003622 lprint([seq(w3(i,j,k,n),n=1..100)]);
%p A003622 od: od: od:
%t A003622 With[{c=GoldenRatio^2},Table[Floor[n c]-1,{n,70}]] (* _Harvey P. Dale_, Jun 11 2011 *)
%t A003622 Range[70]//Floor[#*GoldenRatio^2]-1& (* _Waldemar Puszkarz_, Oct 10 2017 *)
