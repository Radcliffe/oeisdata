%o A035318 seq(N) = {
%o A035318   my(a = vector(N)); a[1] = a[2] = 1;
%o A035318   for (n = 3, N,
%o A035318        a[n] = ((4*n-2)*(n%2)*a[n-1] + (n-1)*(2*n-3)*(2*n-1)*a[n-2])/(n+1));
%o A035318   concat(1,a);
