%t A173005 Clear[f, c, a, t];
%t A173005 f[0, a_] := 0; f[1, a_] := 1;
%t A173005 f[n_, a_] := f[n, a] = (2*a + 1)*f[n - 1, a] - f[n - 2, a];
%t A173005 c[n_, a_] := If[n == 0, 1, Product[f[i, a], {i, 1, n}]];
