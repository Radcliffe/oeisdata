%p A309347 isref:= proc(n) option remember; n mod numtheory:-tau(n) = 0 end proc:
%p A309347 filter:= proc(n)
%p A309347   ormap(t -> isref(t) and isref(n-t) and isref(n-2*t), [$1 .. (n-1)/2])
