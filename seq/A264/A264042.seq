%o A264042 (PARI) A264042()={for (n=1,10000,if(n<3,q=binomial(2*n,n)/(n+1)); if(n>2,q=2*qold1*(8*qold2+qold1)/(10*qold2-qold1)); qold2=qold1; qold1=q; if(ispseudoprime(q-1),print1(q-1,"," )); if(ispseudoprime(q+1),print1(q+1,"," )));}
