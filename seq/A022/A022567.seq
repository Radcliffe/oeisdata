%t A022567 a[ n_] := SeriesCoefficient[ QPochhammer[ q, q^2]^-2, {q, 0, n}]; (* _Michael Somos_, Jul 11 2011 *)
%t A022567 a[ n_] := SeriesCoefficient[ Product[ 1 + q^k, {k, n}]^2, {q, 0, n}]; (* _Michael Somos_, Jul 11 2011 *)
%t A022567 (QPochhammer[-1, x]^2/4 + O[x]^30)[[3]] (* _Vladimir Reshetnikov_, Sep 22 2016 *)
%t A022567 nmax = 50; poly = ConstantArray[0, nmax+1]; poly[[1]] = 1; poly[[2]] = 2; poly[[3]] = 1; Do[Do[Do[poly[[j+1]] += poly[[j-k+1]], {j, nmax, k, -1}]; , {p, 1, 2}], {k, 2, nmax}]; poly (* _Vaclav Kotesovec_, Jan 14 2017 *)
%o A022567 (PARI) {a(n) = if( n<0, 0, polcoeff( prod( k=1, n, 1 + x^k, 1 + x * O(x^n))^2, n))}; /* _Michael Somos_, Mar 21 2004 */
%o A022567 (PARI) {a(n) = my(A); if( n<0, 0, A = x * O(x^n); polcoeff( (eta(x^2 + A) / eta(x + A))^2, n))}; /* _Michael Somos_, Jun 03 2005 */
