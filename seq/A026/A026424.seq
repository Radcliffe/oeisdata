%o A026424 (Python)
%o A026424 from math import isqrt, prod
%o A026424 from sympy import primerange, integer_nthroot, primepi
%o A026424 def A026424(n):
%o A026424     def g(x,a,b,c,m): yield from (((d,) for d in enumerate(primerange(b,isqrt(x//c)+1),a)) if m==2 else (((a2,b2),)+d for a2,b2 in enumerate(primerange(b,integer_nthroot(x//c,m)[0]+1),a) for d in g(x,a2,b2,c*b2,m-1)))
%o A026424     def f(x): return int(n+1+sum(sum(primepi(x//prod(c[1] for c in a))-a[-1][0] for a in g(x,0,1,1,m)) for m in range(2,x.bit_length()+1,2)))
%o A026424     m, k = n, f(n)
%o A026424     while m != k: m, k = k, f(k)
%o A026424     return m # _Chai Wah Wu_, Apr 10 2025
