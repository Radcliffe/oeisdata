%t A318776 t[0, 0] = 1; t[n_, k_] := t[n, k] = If[n < 0 || k < 0, 0, 2 t[n - 1, k] + t[n - 5, k - 1]]; Table[t[n, k], {n, 0, 21}, {k, 0, Floor[n/5]}] // Flatten
