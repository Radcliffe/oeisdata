%o A179879 (PARI) ah(n) = {my(f = factor(n)); if(n == 1, 1, 2*n/3 + (1/3) * prod(i = 1, #f~, 1 - f[i, 1])/eulerphi(f));}
%o A179879 isok(k) = {my(ah1 = ah(k), ah2 = ah(k+1)); ah1 == ah2 && denominator(ah1) > 1;} \\ _Amiram Eldar_, May 24 2025
