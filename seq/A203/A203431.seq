%t A203431 f[j_]:= j + Floor[j/2]; z = 20;
%t A203431 v[n_]:= Product[Product[f[k] - f[j], {j,k-1}], {k,2,n}]
%t A203431 d[n_]:= Product[(i-1)!, {i,n}]
