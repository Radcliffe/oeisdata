%t A126563 h=0; Do[ h=h+1/(n+1)/(2n+1); f=Numerator[n*h]; g=Numerator[h]; If[ !Equal[f, g] && !PrimeQ[f/g], Print[ {n, f/g, FactorInteger[n], FactorInteger[f/g]} ] ], {n, 1, 10000} ]
%o A126563 (PARI) f(n) = sum(k=1, n, 1/(n+k));
