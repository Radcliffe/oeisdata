%t A125790 T[n_, k_] := T[n, k] = T[n, k-1] + T[n-1, 2*k]; T[0, _] = T[_, 0] = 1; Table[T[n-k, k], {n, 0, 10}, {k, 0, n}] // Flatten (* _Jean-Fran√ßois Alcover_, Jun 15 2015 *)
