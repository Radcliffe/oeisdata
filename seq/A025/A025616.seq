%o A025616 import Data.Set (singleton, deleteFindMin, insert)
%o A025616 a025616 n = a025616_list !! (n-1)
%o A025616 a025616_list = f $ singleton (1,0,0) where
%o A025616    f s = y : f (insert (3 * y, i + 1, j) $ insert (10 * y, i, j + 1) s')
