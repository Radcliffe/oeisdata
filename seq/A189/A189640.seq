%t A189640 t = Nest[Flatten[# /. {0->{0,0,1}, 1->{1,0,1}}] &, {0}, 5] (*A189640*)
%t A189640 f[n_] := t[[n]]
%t A189640 Flatten[Position[t, 0]] (*A026138*)
%t A189640 Flatten[Position[t, 1]] (*A026232*)
%t A189640 s[n_] := Sum[f[i], {i, 1, n}]; s[0] = 0;
%t A189640 Table[s[n], {n, 1, 120}] (*A189641*)
