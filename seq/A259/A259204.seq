%N A259204 E.g.f.: Sum_{n>=0} x^n * (1 + x^n)^n * exp(4*x^(n+1)) / n!.
%F A259204 E.g.f.: Sum_{n>=0} x^n * (4 + x^n)^n * exp(x^(n+1)) / n!.
%e A259204 E.g.f.: A(x) = 1 + 5*x + 19*x^2/2! + 89*x^3/3! + 377*x^4/4! + 2225*x^5/5! +...
%e A259204 where
%e A259204 A(x) = exp(4*x) + x*(1+x)*exp(4*x^2) + x^2*(1+x^2)^2*exp(4*x^3)/2! + x^3*(1+x^3)^3*exp(4*x^4)/3! + x^4*(1+x^4)^4*exp(4*x^5)/4! + x^5*(1+x^5)^5*exp(4*x^6)/5! +...
%e A259204 also
%e A259204 A(x) = exp(x) + x*(4+x)*exp(x^2) + x^2*(4+x^2)^2*exp(x^3)/2! + x^3*(4+x^3)^3*exp(x^4)/3! + x^4*(4+x^4)^4*exp(x^5)/4! + x^5*(4+x^5)^5*exp(x^6)/5! +...
%o A259204 (PARI) {a(n) = local(A=1); A = sum(m=0,n,x^m/m!*(1 + x^m +x*O(x^n))^m * exp(4*x^(m+1) +x*O(x^n))); n!*polcoeff(A,n)}
%o A259204 for(n=0,30,print1(a(n),", "))
