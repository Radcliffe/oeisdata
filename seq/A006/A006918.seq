%p A006918 A006918 := proc(n)
%p A006918     if type(n,'even') then
%p A006918         n*(n+2)*(n+4)/24 ;
%p A006918     else
%p A006918         binomial(n+3,3)/4 ;
%p A006918     fi ;
%p A006918 end proc: # _R. J. Mathar_, May 17 2016
%t A006918 f[n_]:=If[EvenQ[n],(n(n+2)(n+4))/24,Binomial[n+3,3]/4]; Join[{0},Array[f,60]]  (* _Harvey P. Dale_, Apr 20 2011 *)
%t A006918 durf[ptn_]:=Length[Select[Range[Length[ptn]],ptn[[#]]>=#&]];
%t A006918 Table[Length[Select[IntegerPartitions[n],durf[#]==2&]],{n,0,30}] (* _Gus Wiseman_, Apr 06 2019 *)
%o A006918 (PARI) { parttrees(n)=local(pt,k,nk); if (n%2==0, pt=(n/2+1)^2, pt=ceil(n/2)*(ceil(n/2)+1)); pt+=floor(n/2); for (x=1,floor(n/2),pt+=floor(x/2)+floor((n-x)/2)); if (n%2==0 && n>2, pt-=floor(n/4)); k=1; while (3*k<=n, for (x=k,floor((n-k)/2), pt+=floor(k/2); if (x!=k, pt+=floor(x/2)); if ((n-x-k)!=k && (n-x-k)!=x, pt+=floor((n-x-k)/2))); k++); pt }
%o A006918 (PARI) {a(n) = n += 2; (n^3 - n * (2-n%2)^2) / 24}; /* _Michael Somos_, Aug 15 2009 */
