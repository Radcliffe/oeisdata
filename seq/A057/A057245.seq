%t A057245 a = 0; b = 1; k = 2; While[k < 250001, c = k*b - (k - 1) a;
%t A057245 If[ Mod[c, k] == 1, Print[k]]; a = b; b = c; k++] (* _Robert G. Wilson v_, Jun 15 2013 *)
