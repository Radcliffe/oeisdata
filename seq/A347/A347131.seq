%t A347131 f[p_, e_] := e/p; d[1] = 0; d[n_] := n * Plus @@ f @@@ FactorInteger[n]; a[n_] := DivisorSum[n, d[#] * EulerPhi[n/#] &]; Array[a, 100] (* _Amiram Eldar_, Sep 03 2021 *)
