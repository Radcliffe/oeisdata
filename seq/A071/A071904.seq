%p A071904     if n = 1 then
%p A071904         9;
%p A071904     else
%p A071904         for a from procname(n-1)+2 by 2 do
%p A071904             if not isprime(a) then
%p A071904                 return a;
%p A071904             end if;
%p A071904         end do:
%p A071904     end if;
%p A071904 end proc: # _R. J. Mathar_, Sep 09 2015
