%t A237826 z = 64; q[n_] := q[n] = IntegerPartitions[n];
%t A237826 Table[Count[q[n], p_ /; 3 Min[p] == Max[p]], {n, z}]     (* A237825*)
%t A237826 Table[Count[q[n], p_ /; 4 Min[p] == Max[p]], {n, z}]     (* A237826 *)
