%t A154269 nn = 95;a = PadRight[{1, 1}, nn, 0];Inverse[Table[Table[If[Mod[n, k] == 0, a[[n/k]], 0], {k, 1, nn}], {n, 1, nn}]][[All, 1]] (* _Mats Granvik_, Jul 24 2017 *)
