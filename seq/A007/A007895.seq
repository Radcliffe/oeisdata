%o A007895 (Python)
%o A007895 from sympy import fibonacci
%o A007895 def a(n):
%o A007895     k=0
%o A007895     x=0
%o A007895     while n>0:
%o A007895         k=0
%o A007895         while fibonacci(k)<=n: k+=1
%o A007895         x+=10**(k - 3)
%o A007895         n-=fibonacci(k - 1)
%o A007895     return str(x).count("1")
%o A007895 print([a(n) for n in range(101)]) # _Indranil Ghosh_, Jun 09 2017
