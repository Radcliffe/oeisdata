%o A101082 (Python)
%o A101082 def A101082(n):
%o A101082     def f(x): return n+((k:=x.bit_length())*(k-1)>>1)+sum(1 for i in range(k) if (1<<k)-(1<<i)<=x)
%o A101082     m, k = n, f(n)
%o A101082     while m != k: m, k = k, f(k)
%o A101082     return m # _Chai Wah Wu_, Feb 23 2025
