%t A348142 (* function a341969 is defined in A341969 *)
%t A348142 a348142[n_, {w_, p_}] := Module[{list=Table[0, {i, w}, {j, p}], k, s, c, u}, For[k=1, k<=n, k++, s=Map[Max, Select[SplitBy[a341969[k], #!=0&], #[[1]]!=0&]]; c=Length[s]; u=Union[s]; If[Length[u]==1&&u[[1]]<=w&&c<=p, If[list[[u[[1]], c]]==0, list[[u[[1]], c]]=k]]]; list]
%t A348142 table=a348142[12000000}, {10, 10}] (* 10x10 table; very long computation time *)
%t A348142 p[n_] := n-row[n-1](row[n-1]+1)/2
