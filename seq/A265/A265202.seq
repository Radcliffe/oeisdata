%p A265202 b:= proc(p, g, n, i, t) option remember; `if`(g=0, 0, p!/g!)+
%p A265202       `if`(n<t, 0, add(b(p+1, `if`(i=j, g+1, 1), n-max(j, t), j,
%p A265202        max(j+1, t+1))/`if`(i=j, 1, g!), j=i..n))
%p A265202     end:
%p A265202 a:= n-> `if`(n=0, 1, b(0$2, n, 1$2)):
%p A265202 seq(a(n), n=0..50);
%t A265202 b[p_, g_, n_, i_, t_] := b[p, g, n, i, t] = If[g==0, 0, p!/g!] + If[n<t, 0, Sum[b[p+1, If[i==j, g+1, 1], n-Max[j, t], j, Max[j+1, t+1]]/If[i==j, 1, g!], {j, i, n}]]; a[n_] := If[n==0, 1, b[0, 0, n, 1, 1]]; Table[a[n], {n, 0, 50}] (* _Jean-FranÃ§ois Alcover_, Feb 02 2017, translated from Maple *)
