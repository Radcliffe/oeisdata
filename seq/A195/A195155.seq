%o A195155 from itertools import pairwise
%o A195155 from sympy import divisors
%o A195155 def A195155(n): return 1 if n&1 else 1+sum(1 for a, b in pairwise(divisors(n)) if a+1==b) # _Chai Wah Wu_, Jun 09 2025
