%N A259203 E.g.f.: Sum_{n>=0} x^n * (1 + x^n)^n * exp(3*x^(n+1)) / n!.
%F A259203 E.g.f.: Sum_{n>=0} x^n * (3 + x^n)^n * exp(x^(n+1)) / n!.
%e A259203 E.g.f.: A(x) = 1 + 4*x + 12*x^2/2! + 46*x^3/3! + 178*x^4/4! + 964*x^5/5! +...
%e A259203 where
%e A259203 A(x) = exp(3*x) + x*(1+x)*exp(3*x^2) + x^2*(1+x^2)^2*exp(3*x^3)/2! + x^3*(1+x^3)^3*exp(3*x^4)/3! + x^4*(1+x^4)^4*exp(3*x^5)/4! + x^5*(1+x^5)^5*exp(3*x^6)/5! +...
%e A259203 also
%e A259203 A(x) = exp(x) + x*(3+x)*exp(x^2) + x^2*(3+x^2)^2*exp(x^3)/2! + x^3*(3+x^3)^3*exp(x^4)/3! + x^4*(3+x^4)^4*exp(x^5)/4! + x^5*(3+x^5)^5*exp(x^6)/5! +...
%o A259203 (PARI) {a(n) = local(A=1); A = sum(m=0,n,x^m/m!*(1 + x^m +x*O(x^n))^m * exp(3*x^(m+1) +x*O(x^n))); n!*polcoeff(A,n)}
%o A259203 for(n=0,30,print1(a(n),", "))
