%e A374385  ...
%t A374385 nn = 17; f[n_] := DivisorSum[n, MoebiusMu[#] # &]; Flatten[CoefficientList[Table[Denominator[Sum[Sum[f[GCD[n, k]]*x^(n*h + k), {k, 1, n}], {h, 0, Infinity}]], {n, 0, nn}], x]]
