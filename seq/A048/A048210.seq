%o A048210 (Python)
%o A048210 from itertools import count, accumulate, islice
%o A048210 from collections import deque
%o A048210 def A048210_gen(): # generator of terms
%o A048210     aset, alist, c = set(), deque(), 0
%o A048210     for k in count(1):
%o A048210         if k in aset:
%o A048210             aset.remove(k)
%o A048210         else:
%o A048210             yield (c:=c+k)+k
%o A048210             aset |= set(k+d for d in accumulate(alist))
%o A048210             alist.appendleft(k)
%o A048210 A048210_list = list(islice(A048210_gen(),45)) # _Chai Wah Wu_, Sep 02 2025
