%o A050446 from functools import cache
%o A050446 @cache
%o A050446 def T(n, k):
%o A050446     return T(n, k - 1) + sum(T(2 * j, k - 1) * T(n - 1 - 2 * j, k)
%o A050446         for j in range(1 + (n - 1) // 2)) if k > 0 else 1
%o A050446 for n in range(6): print([T(n - k, k) for k in range(n + 1)])
