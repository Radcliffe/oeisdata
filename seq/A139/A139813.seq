%t A139813 T[n_, m_, d_] := If[n - m == 0, Binomial[d, n], If[d - n - m == 0, Binomial[d, m], 0]];
%t A139813 M[d_] := Table[T[n, m, d], {n, 0, d}, {m, 0, d}]; p[x_, y_, d_] := Sum[Sum[M[d][[k, m]]*x^(k - 1)*y^(m - 1), {m, 1, d + 1}], {k, 1, d + 1}];
