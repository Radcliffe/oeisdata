%t A360013 q[n_] := Module[{e = IntegerExponent[n, 2], m}, m = n/2^e; (m == 1 && e > 0) || AllTrue[FactorInteger[m][[;; , 2]], # < e &]]; Select[Range[256], q]
%o A360013 (PARI) is(n) = {my(e = valuation(n, 2), m = n >> e); (m == 1 && e > 0) || (m > 1 && vecmax(factor(m)[,2]) < e)};
