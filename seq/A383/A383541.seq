%t A383541 Module[{x, y, runningMax = 0, positions = {}},
%t A383541   x = Range[1, 10^6]; y = Cos[x]^x;
%t A383541   Do[If[y[[i]] > runningMax, runningMax = y[[i]]; AppendTo[positions, i]; ], {i, Length[y]}];
