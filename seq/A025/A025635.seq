%o A025635 (Haskell)
%o A025635 import Data.Set (singleton, deleteFindMin, insert)
%o A025635 a025635 n = a025635_list !! (n-1)
%o A025635 a025635_list = f $ singleton (1,0,0) where
%o A025635    f s = y : f (insert (9 * y, i + 1, j) $ insert (10 * y, i, j + 1) s')
