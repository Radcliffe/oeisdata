%p A389115 q:= p-> isprime(p) and (h-> h<p-1 and irem(h, 2)=0 and isprime(h/2))(numtheory[order](2, p)):
%p A389115 select(q, [$2..10000])[];  # _Alois P. Heinz_, Sep 24 2025
%o A389115 (PARI)
%o A389115 isok(p) = if (isprime(p) && (p>2),  my(o = znorder(Mod(2, p))); (o%2==0) && isprime(o/2) && (o < p-1));
%o A389115 select(isok, primes(2000))
%o A389115 (Python)
%o A389115 from sympy import n_order, isprime
%o A389115 def ok(p): return isprime(p) and p>2 and (o:=n_order(2, p))&1==0 and o!=p-1 and isprime(o>>1)
%o A389115 print([k for k in range(1, 10244) if ok(k)]) # _Michael S. Branicky_, Sep 25 2025
