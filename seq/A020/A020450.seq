%o A020450 def checkd(a, c):
%o A020450     b =  set(int(i) for i in set(str(a)))
%o A020450     return b.issubset(c)
%o A020450 for n in primerange(2, 2000000):
%o A020450     if checkd(n, [1, 2]):
%o A020450         print(n)
