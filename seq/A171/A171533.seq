%t A171533 Clear[A, p, n, k]
%t A171533 m = 2;
%t A171533 A[n_, 1] := 1 A[n_, n_] := 1
%t A171533 A[n_, k_] := (m*n - m*k + 1)A[n - 1, k - 1] + (m*k - (m - 1))A[n - 1, k]
