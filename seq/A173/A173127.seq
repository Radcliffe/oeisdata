%o A173127 from itertools import islice
%o A173127 def A173127_gen(): # generator of terms
%o A173127     x, y = -30, 10
%o A173127     while True:
%o A173127         yield x//10
%o A173127         x, y = x*19+y*60, x*6+y*19
