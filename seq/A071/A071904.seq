%o A071904 (Python)
%o A071904 from sympy import primepi
%o A071904 def A071904(n):
%o A071904     if n == 1: return 9
%o A071904     m, k = n, primepi(n) + n + (n>>1)
%o A071904     while m != k:
%o A071904         m, k = k, primepi(k) + n + (k>>1)
%o A071904     return m # _Chai Wah Wu_, Jul 31 2024
