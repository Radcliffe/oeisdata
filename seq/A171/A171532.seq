%t A171532 Clear[A, p, n, k]
%t A171532 m = 1;
%t A171532 A[n_, 1] := 1 A[n_, n_] := 1
%t A171532 A[n_, k_] := (m*n - m*k + 1)A[n - 1, k - 1] + (m*k - (m - 1))A[n - 1, k]
%t A171532 a = Table[A[n, k], {n, 10}, {k, n}]
