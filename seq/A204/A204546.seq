%t A204546   {n, 1, 14}, {i, 1, n}]]     (* A204545 *)
%t A204546 Permanent[m_] :=
%t A204546   With[{a = Array[x, Length[m]]},
%t A204546    Coefficient[Times @@ (m.a), Times @@ a]];
