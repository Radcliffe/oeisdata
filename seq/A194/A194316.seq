%e A194316   31  33  32  33  30  33  31  33
%t A194316 r = Sqrt[6];
%t A194316 f[n_, k_, i_] := If[(k - 1)/n <= FractionalPart[i*r] < k/n, 1, 0]
%t A194316 g[n_, k_] := Sum[f[n, k, i], {i, 1, 2^n}]
%t A194316 TableForm[Table[g[n, k], {n, 1, 14}, {k, 1, n}]]
