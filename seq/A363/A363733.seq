%p A363733 divides := (k, n) -> ifelse(k = n or (k > 0 and irem(n, k) = 0), 1, 0):
%p A363733 A := (n, k) -> local j; add(divides(j, n) * k^j, j = 0 ..n):
%p A363733 for n from 0 to 8 do seq(A(n, k), k = 0..8) od;
%p A363733 # If we introduce the 'inverse Möbius transform' InvMoebius acting on s ...
%p A363733 InvMoebius := (s, n) -> local j; add(divides(j, n) * s(j), j = 0 ..n):
%p A363733 # ... the transposed array is given by applying InvMoebius to the powers r^m:
%p A363733 seq(lprint(seq(InvMoebius(m -> r^m, n), n = 0..8)), r = 0..8);
%p A363733 # For instance we see that the number of divisors is the inverse
%p A363733 # Moebius transform of the constant sequence s = 1.
%o A363733 (SageMath)
%o A363733 def A(n, k): return sum(j.divides(n) * k^j for j in (0..n))
%o A363733 for n in srange(9): print([A(n, k) for k in (0..8)])
