%e A143363   o  (o)  ((o))  ((o)o)   (((o))o)
%e A143363                  (o(o))   (((o)o))
%e A143363                  (((o)))  ((o)(o))
%e A143363                           ((o(o)))
%e A143363                           (o((o)))
%e A143363                           ((((o))))
%e A143363 (End)
