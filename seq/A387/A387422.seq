%F A387422 a(n) = (1+A000523(n)) - A387423(n).
%o A387422 (PARI) A387422(n) = { my(a=binary(n), b=binary(sigma(n)), i=1); while(i<=#a,if(a[i]!=b[i],return(i-1)); i++); (#a); };
