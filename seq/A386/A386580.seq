%t A386580 allnorm[n_]:=If[n<=0,{{}},Function[s,Array[Count[s,y_/;y<=#]+1&,n]]/@Subsets[Range[n-1]+1]];
%t A386580 strconsfans[c_]:=Table[ConstantArray[c[[1]],#]&/@y,{y,Select[IntegerPartitions[Length[c]],UnsameQ@@#&]}];
%t A386580 specialPermutations[y_]:=Union[Join@@@Select[Join@@Permutations/@Join@@@Select[Tuples[strconsfans/@Split[y]],UnsameQ@@Length/@Join@@#&],Length[Split[First/@#]]==Length[#]&]];
%t A386580 Table[Length[Select[allnorm[n],specialPermutations[#]!={}&]],{n,0,10}]
%t A386580 (* - or (conjectured) - *)
%t A386580 allnorm[n_]:=If[n<=0,{{}},Function[s,Array[Count[s,y_/;y<=#]+1&,n]]/@Subsets[Range[n-1]+1]];
%t A386580 pof[y_]:=Select[Join@@@Tuples[IntegerPartitions/@y],UnsameQ@@#&];
%t A386580 Table[Length[Select[allnorm[n],pof[Length/@Split[#]]!={}&]],{n,15}]
