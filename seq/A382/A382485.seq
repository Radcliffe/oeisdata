%N A382485 Least composite squarefree numbers k > n such that p + n divides k - n, for each prime p dividing k.
%e A382485 a(8) = 343027 = 37 * 73 * 127 and
%e A382485   (343027 + 8)/(37 + 8) = 7623;
%e A382485   (343027 + 8)/(73 + 8) = 4235;
%e A382485   (343027 + 8)/(127 + 8) = 2541.
%p A382485 with(numtheory): P:=proc(q) local d, k, ok, n, p;
%p A382485 for n from 1 to 11 do for k from n+1 to q do
%p A382485 if issqrfree(k) and not isprime(k) then p:=factorset(k); ok:=1;
%p A382485 for d from 1 to nops(p) do if frac((k+n)/(p[d]+n))>0 then ok:=0;
%p A382485 break; fi; od; if ok=1 then lprint(n, k); break; fi; fi; od; od; end: P(10^8);
