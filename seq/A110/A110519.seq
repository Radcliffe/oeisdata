%t A110519 T[0, 0] := 1; T[0, k_] := 0; T[n_, k_] := Sum[j*3^(n - j)*Binomial[2*n - j - 1, n - j]*Binomial[j, k]/n, {j, 0, n}]; Table[T[n, k], {n, 0, 9}, {k, 0, n}] // Flatten (* _G. C. Greubel_, Aug 29 2017 *)
