%e A124800   ...
%t A124800 p[x_, n_] = Sum[Prime[k + 1]*Binomial[n, k]*x^k*(1 - x)^(n - k), {k, 0, n}];
%t A124800 Table[CoefficientList[ExpandAll[p[x, n]], x], {n, 0, 10}] // Flatten (* _Roger L. Bagula_ and _Gary W. Adamson_, Oct 01 2008 *)
