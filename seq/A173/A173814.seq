%t A173814 MatrixJoinH[A_, B_] := Transpose[Join[Transpose[A], Transpose[B]]]
%t A173814 KroneckerProduct[M_, N_] := Module[{M1, N1, LM, LN, N2},
%t A173814 M1 = M;
%t A173814 N1 = N;
%t A173814 LM = Length[M1];
%t A173814 LN = Length[N1];
%t A173814 Do[M1[[i, j]] = M1[[i, j]]N1, {i, 1, LM}, {j, 1, LM}];
%t A173814 Do[M1[[i, 1]] = MatrixJoinH[M1[[i, 1]], M1[[i, j]]], {j, 2, LM}, {i, 1, LM}];
%t A173814 N2 = {};
%t A173814 Do[AppendTo[N2, M1[[i, 1]]], {i, 1, LM}];
%t A173814 N2 = Flatten[N2];
%t A173814 Partition[N2, LM*LN, LM*LN]]
%t A173814 HadamardMatrix[2] := {{2, -1}, {-3, 2}}
%t A173814 HadamardMatrix[n_] := Module[{m},
%t A173814 m = {{2, -1}, {-3, 2}};
%t A173814 KroneckerProduct[m, HadamardMatrix[n/2]]]
%t A173814 Table[HadamardMatrix[2^n], {n, 1, 4}]
%t A173814 Join[{{1}}, Table[CoefficientList[CharacteristicPolynomial[ HadamardMatrix[2^n], x], x], {n, 1, 6}]]
