%p A285183 Digits:=30;
%p A285183 A001221 := proc(n) nops(numtheory[factorset](n)) end proc:
%p A285183 with(numtheory);
%p A285183 f:=n->round(n*A001221(n)/phi(n));
%p A285183 t1:=[seq(f(n),n=1..130)];
