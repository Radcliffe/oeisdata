%p A005101 with(numtheory): for n from 1 to 270 do if sigma(n)>2*n then printf(`%d,`,n) fi: od:
%p A005101 isA005101 := proc(n)
%p A005101     simplify(numtheory[sigma](n) > 2*n) ;
%p A005101 end proc: # _R. J. Mathar_, Jun 18 2015
%p A005101 A005101 := proc(n)
%p A005101     option remember ;
%p A005101     local a ;
%p A005101     if n =1 then
%p A005101         12 ;
%p A005101     else
%p A005101         a := procname(n-1)+1 ;
%p A005101         while numtheory[sigma](a) <= 2*a do
%p A005101             a := a+1 ;
%p A005101         end do ;
%p A005101         a ;
%p A005101     end if ;
%p A005101 end proc: # _R. J. Mathar_, Oct 11 2017
