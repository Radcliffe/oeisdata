%t A086086 f[n_]:=n-Floor[Sqrt[n]];lst={};Do[p=Prime[n];If[PrimeQ[f[p]],AppendTo[lst,p]],{n,7!}];lst (* _Vladimir Joseph Stephan Orlovsky_, Feb 25 2010 *)
