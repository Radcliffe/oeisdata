%o A038444 (Haskell)
%o A038444 a038444 n = a038444_list !! (n-1)
%o A038444 a038444_list = 11 : f [11] 90 where
%o A038444    f xs@(x:_) z = ys ++ f ys (10 * z) where
%o A038444                   ys = (x + z) : map (* 10) xs
