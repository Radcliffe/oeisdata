%F A353357 {a(n) : n >= 1} = {m : tau(m) * A048675(m) == 1 (mod 3)}, where tau is the number of divisors function, A000005.
%o A353357 (PARI)
%o A353357 A332823(n) = { my(f = factor(n),u=(sum(k=1, #f~, f[k, 2]*2^primepi(f[k, 1]))/2)%3); if(2==u,-1,u); };
%o A353357 A353354(n) = sumdiv(n,d,A332823(d));
%o A353357 isA353357(n) = (0>A353354(n));
%o A353357 (PARI)
%o A353357 A048675(n) = { my(f = factor(n)); sum(k=1, #f~, f[k, 2]*2^primepi(f[k, 1]))/2; };
%o A353357 isA353357(n) = (1==((numdiv(n)*A048675(n))%3));
