%o A060176 (PARI)
%o A060176 up_to = 105;
%o A060176 A060176sq(n,k) = (prime(k)^valuation(n,prime(k)));
%o A060176 A060176list(up_to) = { my(v = vector(up_to), i=0); for(a=1,oo, for(col=1,a, i++; if(i > up_to, return(v)); v[i] = A060176sq(col,(a-(col-1))))); (v); };
