%C A184927   A184927: 1,4,7,10,13,16,18,22,24,...
%C A184927 Jointly rank the sets {h*r}, {i*s}, {j*t}, {k*u},
%C A184927 where h>=1, i>=1, j>=1, k>=1.  The position of n*u in the joint ranking is n+[rn/u]+[sn/u]+[tn/u], and likewise for the positions of n*r, n*s, and n*t.
%t A184927 r=1; s=3^(1/2); t=5^(1/2); u=7^(1/2);
%t A184927 a[n_]:=n+Floor[n*s/r]+Floor[n*t/r]+Floor[n*u/r];
%t A184927 b[n_]:=n+Floor[n*r/s]+Floor[n*t/s]+Floor[n*u/s];
%t A184927 c[n_]:=n+Floor[n*r/t]+Floor[n*s/t]+Floor[n*u/t];
%t A184927 d[n_]:=n+Floor[n*r/u]+Floor[n*s/u]+Floor[n*t/u];
%t A184927 Table[a[n],{n,1,120}]  (* A184924 *)
%t A184927 Table[b[n],{n,1,120}]  (* A184925 *)
