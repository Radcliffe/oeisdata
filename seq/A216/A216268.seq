%p A216268 select(t -> issqr(t+1), [seq(i*(i+1)*(i+2)/6, i=0..10^6)]); # _Robert Israel_, Jan 02 2024
%t A216268 t = {}; Do[tet = n (n + 1) (n + 2)/6; If[IntegerQ[Sqrt[tet + 1]], AppendTo[t, tet]], {n, 0, 100000}]; t (* _T. D. Noe_, Mar 18 2013 *)
