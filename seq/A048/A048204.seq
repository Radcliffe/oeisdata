%o A048204 (Python)
%o A048204 from itertools import count, accumulate, islice
%o A048204 from collections import deque
%o A048204 def A048204_gen(): # generator of terms
%o A048204     aset, alist, c = {1}, deque([1]), 0
%o A048204     for k in count(2):
%o A048204         if k in aset:
%o A048204             aset.remove(k)
%o A048204         else:
%o A048204             yield (c:=c+k)
%o A048204             aset |= set(k+d for d in accumulate(alist))
%o A048204             alist.appendleft(k)
%o A048204 A048204_list = list(islice(A048204_gen(),50)) # _Chai Wah Wu_, Sep 01 2025
