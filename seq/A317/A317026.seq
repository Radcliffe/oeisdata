%t A317026 t[0, 0] = 1; t[n_, k_] := If[n < 0 || k < 0, 0, t[n - 1, k] + 8 t[n - 2, k - 1]]; Table[t[n, k], {n, 0, 13}, {k, 0, Floor[n/2]}] // Flatten
%t A317026 Table[8^k Binomial[n - k, k], {n, 0, 13}, {k, 0, Floor[n/2]}] // Flatten
