%o A269391 (definec (A269391 n) (cond ((<= n 1) n) ((zero? (- (A269381 n) (A269381 (- n 1)))) (+ 1 (* 2 (A269391 (- n (A269381 n)))))) (else (* 2 (A269391 (+ -1 (A269381 n)))))))
