%p A194094 t3:= curry(JacobiTheta3,0):
%p A194094 t4:= curry(JacobiTheta4,0):
%p A194094 Dq:= f -> q*diff(f,q):
%p A194094 E1:= t3(q)^2:
%p A194094 E2a:= - 2*(t4(q)/t3(q))^2:
%p A194094 E2b:= t4(q)^(-2):
%p A194094 S1:= series(E1,q,N+1):
