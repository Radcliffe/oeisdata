%p A358914 g:= proc(n) option remember; `if`(n=0, 1, add(g(n-j)*add(
%p A358914      `if`(d::odd, d, 0), d=numtheory[divisors](j)), j=1..n)/n)
%p A358914     end:
%p A358914 b:= proc(n, i) option remember; `if`(n=0, 1, `if`(i<1, 0,
%p A358914       add(binomial(g(i), j)*b(n-i*j, i-1)*j!, j=0..n/i)))
%p A358914     end:
%p A358914 a:= n-> b(n$2):
%p A358914 seq(a(n), n=0..45);  # _Alois P. Heinz_, Sep 08 2025
%t A358914 twiptn[n_]:=Join@@Table[Tuples[IntegerPartitions/@ptn],{ptn,IntegerPartitions[n]}];
%t A358914 Table[Length[Select[twiptn[n],UnsameQ@@#&&And@@UnsameQ@@@#&]],{n,0,10}]
%o A358914 (PARI) seq(n,k)={my(u=Vec(eta(x^2 + O(x*x^n))/eta(x + O(x*x^n))-1)); Vec(prod(k=1, n, my(c=u[k]); sum(j=0, min(c,n\k), x^(j*k)*c!/(c-j)!,  O(x*x^n))))} \\ _Andrew Howroyd_, Dec 31 2022
