%o A269701 fibocy(N) when N > 1 ->
%o A269701    Tmp = fibocy(N-1) + fibocy(N-2),
%o A269701    if Tmp > 6 -> Tmp - 6;
%o A269701       true -> Tmp
%o A269701    end.
