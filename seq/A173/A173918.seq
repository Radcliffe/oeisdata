%t A173918 Clear[t, n, m, c, q, w, f, a];
%t A173918 f[0, a_] := 0; f[1, a_] := 1;
%t A173918 f[n_, a_] := f[n, a] = f[n - 1, a] + a*f[n - 2, a];
%t A173918 c[n_, a_] := If[n == 0, 1, Product[f[i, a]*f[i + 1, a], {i, 1, n}]];
%t A173918 w[n_, m_, q_] := c[n, q]/(c[m, q]*c[n - m, q]);
%t A173918 Table[Table[Table[w[n, m, q], {m, 0, n}], {n, 0, 10}], {q, 1, 12}];
