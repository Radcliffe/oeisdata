%o A048679 (Python)
%o A048679 def A048679(n):
%o A048679     tlist, s = [1,2], 0
%o A048679     while tlist[-1]+tlist[-2] <= n: tlist.append(tlist[-1]+tlist[-2])
%o A048679     for d in tlist[::-1]:
%o A048679         if d <= n:
%o A048679             s += 1
%o A048679             n -= d
%o A048679         else:
%o A048679             s <<= 1
%o A048679     return s # _Chai Wah Wu_, Apr 24 2025
