%t A182237 nn = 1000; t = Table[s = {}; k = 1; While[k++; b = Binomial[n, k]; k <= n/2 && b <= nn, AppendTo[s, b]]; s, {n, nn}]; t2 = Select[t, Length[#] > 0 &]; Transpose[Select[Tally[Sort[Flatten[t2]]], #[[2]] == 1 &]][[1]] (* _T. D. Noe_, Mar 13 2013 *)
