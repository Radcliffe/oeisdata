%o A006286 (Python)
%o A006286 from itertools import count, islice, combinations
%o A006286 from sympy import isprime
%o A006286 def A006286_gen(startvalue=1): # generator of terms >= startvalue
%o A006286     for i in count(max(startvalue,1)):
%o A006286         l = i.bit_length()
%o A006286         for j in range(1,l):
%o A006286             if isprime(i-(1<<j)):
%o A006286                 break
%o A006286         else:
%o A006286             for p in combinations(range(l),2):
%o A006286                 if isprime(i-(1<<p[0])-(1<<p[1])):
%o A006286                     break
%o A006286             else:
%o A006286                 yield i
%o A006286 A006286_list = list(islice(A006286_gen(),55)) # _Chai Wah Wu_, Sep 12 2025
