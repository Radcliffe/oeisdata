%p A241619 for m from 1 to 12 do
%p A241619   r:= [seq(seq([i,j],j=0..m-i),i=0..m)];
%p A241619   T[m]:= Matrix((m+1)*(m+2)/2,(m+1)*(m+2)/2, proc(i, j) if r[i][1]=r[j][2] and r[i][1]+r[i][2]+r[j][1]<=m then 1 else 0 fi end proc):
%p A241619   U[m,0]:= Vector((m+1)*(m+2)/2,1);
%p A241619 od:
%p A241619 R:= NULL:
%p A241619 for i from 2 to 12 do
%p A241619   for j from 1 to i-1 do
%p A241619     U[i-j,j]:= T[i-j] . U[i-j,j-1];
%p A241619     R:= R, convert(U[i-j,j],`+`)
%p A241619 od od:
