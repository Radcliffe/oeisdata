%p A079823 A079823 := proc(n)
%p A079823     local L,k,n0 ;
%p A079823     n0 := n-1 ;
%p A079823     L := [] ;
%p A079823     for k from 0 do
%p A079823         if k > n0-k then
%p A079823             break;
%p A079823         end if;
%p A079823         L := [op(L),A079823aux(n0-k,k)] ;
%p A079823     end do:
%p A079823     digcatL(L) ;
%p A079823 end proc: # _R. J. Mathar_, Aug 23 2012
