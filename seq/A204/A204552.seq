%t A204552   {n, 1, 14}, {i, 1, n}]]    (* A204551 *)
%t A204552 Permanent[m_] :=
%t A204552   With[{a = Array[x, Length[m]]},
%t A204552    Coefficient[Times @@ (m.a), Times @@ a]];
