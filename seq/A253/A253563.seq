%t A253563 p[n_] := p[n] = FactorInteger[n][[-1, 1]];
%t A253563 b[n_] := n p[n];
%t A253563 c[1] = 1; c[n_] := (n/p[n]) NextPrime[p[n]];
%t A253563 a[0] = 1; a[1] = 2; a[n_] := a[n] = If[EvenQ[n], b[a[n/2]], c[a[(n-1)/2]]];
