%p A308620     add( binomial(n-3*i+2,2*i-2) +binomial(floor((n-5*i+4)/2)+i-1,i-1), i=1..floor((n+4)/5)) ;
%p A308620     %+add(binomial(n-3*i-2,2*i)+binomial(floor((n-5*i-2)/2)+i,i), i=1..floor((n-2)/5)) ;
