%p A026424 isA026424 := proc(n)
%p A026424     if type(numtheory[bigomega](n) ,'odd') then
%p A026424         true;
%p A026424     else
%p A026424         false;
%p A026424     end if;
%p A026424 end proc:
%p A026424 A026424 := proc(n)
%p A026424     option remember;
%p A026424     if n =1 then
%p A026424         2;
%p A026424     else
%p A026424         for a from procname(n-1)+1 do
%p A026424             if isA026424(a) then
%p A026424                 return a;
%p A026424             end if;
%p A026424         end do:
%p A026424     end if;
%p A026424 end proc: # _R. J. Mathar_, May 25 2017
