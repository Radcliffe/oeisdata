%p A145271 with(LinearAlgebra): with(ListTools):
%p A145271 A145271_row := proc(n) local b, M, V, U, G, R, T;
%p A145271 if n < 2 then return 1 fi;
%p A145271 b := (n,k) -> `if`(k=1 or k>n+1,0,binomial(n-1,k-2)*g[n-k+1]);
%p A145271 M := n -> Matrix(n, b):
%p A145271 V := n -> Vector[row]([1, seq(0,i=2..n)]):
%p A145271 U := n -> VectorMatrixMultiply(V(n), M(n)^(n-1)):
%p A145271 G := n -> Vector([seq(g[i], i=0..n-1)]);
%p A145271 R := n -> VectorMatrixMultiply(U(n), G(n)):
%p A145271 T := Reverse([op(sort(expand(R(n+1))))]);
%p A145271 seq(subs({seq(g[i]=1, i=0..n)},T[j]),j=1..nops(T)) end:
%p A145271 for n from 0 to 9 do A145271_row(n) od; # _Peter Luschny_, Jul 20 2016
