%T A192311 70504,121828,209663,359535,614576,1047536,1780918,3020688,5112649,
%U A192311 8636617,14563592,24517818,41213348,69180716,115975555,194187315,324775408,542606308,905637842
%N A192311 0-sequence of reduction of (3n-2) by x^2 -> x+1.
%C A192311 See A192232 for definition of "k-sequence of reduction of [sequence] by [substitution]".
%F A192311 Empirical g.f.: x*(1-2*x+6*x^2-2*x^3)/(1-3*x+x^2+3*x^3-x^4-x^5). - _Colin Barker_, Feb 09 2012
%t A192311 c[n_] := 3 n - 2;
%t A192311 Table[c[n], {n, 1, 15}]
%t A192311 q[x_] := x + 1;
%t A192311 p[0, x_] := 1; p[n_, x_] := p[n - 1, x] + (x^n)*c[n + 1]
%t A192311 reductionRules = {x^y_?EvenQ -> q[x]^(y/2), x^y_?OddQ -> x q[x]^((y - 1)/2)};
%t A192311 t = Table[Last[Most[FixedPointList[Expand[#1 /. reductionRules] &, p[n, x]]]], {n, 0, 40}]
%t A192311 Table[Coefficient[Part[t, n], x, 0], {n, 1, 40}]  (* A192311 *)
%t A192311 Table[Coefficient[Part[t, n], x, 1], {n, 1, 40}]  (* A192312 *)
