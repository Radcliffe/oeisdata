%t A367685 mmk[n_Integer]:= Product[Min[#]^Length[#]&[First/@Select[FactorInteger[n], Last[#]==k&]], {k,Union[Last/@FactorInteger[n]]}];
%t A367685 Select[Range[100], Divisible[#,mmk[#]]&]
