%t A367301 p[1, x_] := 1; p[2, x_] := 3 + 3 x; u[x_] := p[2, x]; v[x_] := 1 - 2 x - x^2;
%t A367301 p[n_, x_] := Expand[u[x]*p[n - 1, x] + v[x]*p[n - 2, x]]
