%t A079448 seq[lim_] := Union[Select[Table[k * DivisorSigma[0, k] - 1, {k, 1, Floor[lim/2]+1}], # <= lim && PrimeQ[#] &]]; seq[1000] (* _Amiram Eldar_, Apr 19 2025 *)
