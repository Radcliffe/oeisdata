%p A103628 # second Maple program:
%p A103628 b:= proc(n, i) option remember; `if`(n=0, [1, 0],
%p A103628       `if`(i<1, [0, 0], add((l->`if`(j=1, [l[1],
%p A103628        l[2]+l[1]*i], l))(b(n-i*j, i-1)), j=0..n/i)))
%p A103628     end:
%p A103628 a:= n-> b(n, n)[2]:
%p A103628 seq(a(n), n=0..50);  # _Alois P. Heinz_, Feb 03 2013
%t A103628 b[n_, p_] := b[n, p] = If[n == 0 && p == 0, {1, 0}, If[p == 0, Array[0&, n+2], Sum[Function[l, ReplacePart[l, m+2 -> p*l[[1]] + l[[m+2]]]][Join[b[n-p*m, p-1], Array[0&, p*m]]], {m, 0, n/p}]]]; a[n_] := b[n, n][[3]]; a[0] = 0; Table[a[n], {n, 0, 50}]  (* _Jean-Fran√ßois Alcover_, Jan 24 2014, after _Alois P. Heinz_ *)
