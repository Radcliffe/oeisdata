%N A216362 Primes of the form 2^(k-1)*k^2-(-1)^k.
%C A216362 Generated by k: 1, 2, 3, 4, 5, 6, 7, 8, 10, 12, 14, 15, 20, 25, 43, 47, 85, 187, 188, 206, 275, 292 ...
%C A216362 Primes of the form 2^(k-1)*k^2+(-1)^k are 1153, 44530220924929, ...
%e A216362 a(1) = 2 (= 2^(1 - 1)*1^2 - (-1)^1 = 1 + 1),
%e A216362 a(2) = 7 (= 2^(2 - 1)*2^2 - (-1)^2 = 8 - 1),
%e A216362 a(3) = 37 (= 2^(3 - 1)*3^2 - (-1)^3 = 36 + 1),
%e A216362 a(4) = 127 (= 2^(4 - 1)*4^2 - (-1)^4 = 128 - 1),
%e A216362 a(5) = 401 (= 2^(5 - 1)*5^2 - (-1)^5 = 400 + 1),
%e A216362 a(6) = 1151 (= 2^(6 - 1)*6^2 - (-1)^6 = 1152 - 1),
%e A216362 a(7) = 3137 (= 2^(7 - 1)*7^2 - (-1)^7 = 3136 + 1),
%e A216362 a(8) = 8191 (= 2^(8 - 1)*8^2 - (-1)^8 = 8192 - 1).
%t A216362 a={}; For[n = 1, n <= 300, n++, {v = 2^(n-1)*n^2-(-1)^n; If[PrimeQ[v], AppendTo[a, v]]}]; a (* _John W. Layman_, Sep 06 2012 *)
