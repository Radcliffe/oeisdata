%o A348626 (Python)
%o A348626 from math import isqrt
%o A348626 from fractions import Fraction
%o A348626 def A348626List():
%o A348626     s = Fraction(1, 1)
%o A348626     while True:
%o A348626         t = isqrt(1 // s) + 1
%o A348626         yield t
%o A348626         s -= Fraction(1, t * t)
