%o A247095 (Python)
%o A247095 from itertools import count, islice
%o A247095 from sympy import factorint
%o A247095 def f(n): # A250030
%o A247095     k = 0
%o A247095     while n != 5: n, k = sum(p+e for p, e in factorint(n).items()), k + 1
%o A247095     return k
%o A247095 def agen(): # generator of terms
%o A247095     adict, n = dict(), 1
%o A247095     for k in count(5):
%o A247095         v = f(k)
%o A247095         if v not in adict:
%o A247095             adict[v] = k
%o A247095             while n in adict: yield adict[n]; n += 1
