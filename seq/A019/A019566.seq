%t A019566 f[n_] := Block[ {a = "", k = 1}, While[k < n + 1, a = StringJoin[ ToString[k], a]; k++ ]; Return[ ToExpression[a] - ToExpression[ StringReverse[a]]]]; Table[ f[n], {n, 1, 17} ]
